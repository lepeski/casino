{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.slot_lever": {
      "states": {
        "default": {
          "animations": [],
          "transitions": [
            { "pulled": "variable.slot_machine_lever_pull > 0.5" }
          ]
        },
        "pulled": {
          "animations": [
            "animation.slot_lever.pull"
          ],
          "transitions": [
            { "default": "query.all_animations_finished" }
          ]
        }
      }
    }
  }
}
