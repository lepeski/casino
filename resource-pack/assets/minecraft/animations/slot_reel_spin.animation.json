{
  "format_version": "1.8.0",
  "animations": {
    "animation.slot_reel.spin": {
      "loop": "once",
      "animation_length": 3.0,
      "override_previous_animation": true,
      "bones": {
        "reel": {
          "rotation": {
            "0.0": { "vector": [ 0, 0, 0 ], "easing": "linear" },
            "1.0": { "vector": [ 240, 0, 0 ], "easing": "linear" },
            "2.0": { "vector": [ 480, 0, 0 ], "easing": "linear" },
            "3.0": { "vector": [ 720, 0, 0 ], "easing": "linear" }
          }
        }
      }
    }
  }
}
